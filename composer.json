{
  "name": "revolta77/backupmanager",
  "description": "laravel package to backup/restore files and database.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Dusan Rybar",
      "email": "dusan.rybar7@gmail.com",
    }
  ],
  "homepage": "https://github.com/Revolta77/backupmanager",
  "keywords": [
    "Laravel",
    "Backup",
    "Restore",
    "Database",
    "Server"
  ],
  "require": {
    "illuminate/support": "~5|~6|~7"
  },
  "autoload": {
    "psr-4": {
      "Revolta77\\BackupManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Revolta77\\BackupManager\\ServiceProvider"
      ],
      "aliases": {
        "BackupManager": "Revolta77\\BackupManager\\Facades\\BackupManager"
      }
    }
  },
  "minimum-stability": "stable"
}
